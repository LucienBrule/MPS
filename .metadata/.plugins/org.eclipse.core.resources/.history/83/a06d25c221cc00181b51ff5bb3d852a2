#include "init.h"

// Main Execution Loop
int main(void) {
	//Initialize the system
	Sys_Init();
	HAL_Init();

	UART_HandleTypeDef* usart6;

	initUart(&usart6, 9600, USART6);


	printf("Your code here!\r\n");

	// Lost? Check out uart.c!

}
