/*
 * UART information:
 * 	textbook page 221
 * 	reference manual page 1241
 * 	USART1 and 6 setup in uart.c
 */
#include "init.h"

// Main Execution Loop
int main(void) {
	//Initialize the system
	Sys_Init();

	printf("[printf] before");
	//Initialize GPIO for UART
//	init uart populates a uart struct.
	UART_HandleTypeDef *uart1_ptr;
	initUart(uart1_ptr,9600,USART1);
	printf("[printf] 9600");

	// Lost? Check out uart.c!

}
