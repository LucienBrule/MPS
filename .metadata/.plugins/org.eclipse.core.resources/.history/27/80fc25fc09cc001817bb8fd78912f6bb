/*
 * UART information:
 * 	textbook page 221
 * 	reference manual page 1241
 * 	USART1 and 6 setup in uart.c
 */
#include "init.h"

// Main Execution Loop
int main(void) {
	//Initialize the system
	Sys_Init();
	printf("[printf] before\r\n");

	//Initialize GPIO for UART
//	init uart populates a uart struct.
	while(1){
		UART_HandleTypeDef *uart6_ptr;
		initUart(uart6_ptr,9600,USART6);
		uart_print(uart6_ptr,"[printf] 9600\r\n");
}

	// Lost? Check out uart.c!

}
